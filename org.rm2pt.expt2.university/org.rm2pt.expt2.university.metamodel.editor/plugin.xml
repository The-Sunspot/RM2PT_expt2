<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated university -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.rm2pt.expt2.university.metamodel.university.presentation.UniversityModelWizardID"
            name="%_UI_UniversityModelWizard_label"
            class="org.rm2pt.expt2.university.metamodel.university.presentation.UniversityModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/UniversityModelFile.gif">
         <description>%_UI_UniversityModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated university -->
      <editor
            id="org.rm2pt.expt2.university.metamodel.university.presentation.UniversityEditorID"
            name="%_UI_UniversityEditor_label"
            icon="icons/full/obj16/UniversityModelFile.gif"
            extensions="university"
            class="org.rm2pt.expt2.university.metamodel.university.presentation.UniversityEditor"
            contributorClass="org.rm2pt.expt2.university.metamodel.university.presentation.UniversityActionBarContributor">
      </editor>
   </extension>

</plugin>
